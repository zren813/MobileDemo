<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DisplaySearchResult">

    <TextView
        android:id="@+id/result"
        android:layout_width="110dp"
        android:layout_height="39dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="272dp"
        android:layout_marginBottom="27dp"
        android:text="Result"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/target_name_title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/target_name_title"
        android:layout_width="75dp"
        android:layout_height="24dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="28dp"
        android:text="Name"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/target_email_title"
        app:layout_constraintEnd_toStartOf="@+id/target_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/result" />

    <TextView
        android:id="@+id/target_name"
        android:layout_width="123dp"
        android:layout_height="21dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="143dp"
        android:layout_marginEnd="102dp"
        android:text="TextView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/target_name_title"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/target_email_title"
        android:layout_width="81dp"
        android:layout_height="32dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="505dp"
        android:text="Email"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/target_email"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/target_name_title"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/target_email"
        android:layout_width="155dp"
        android:layout_height="36dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="485dp"
        android:text="TextView"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/target_email_title"
        app:layout_constraintTop_toBottomOf="@+id/target_name"
        app:layout_constraintVertical_bias="1.0" />
</androidx.constraintlayout.widget.ConstraintLayout>